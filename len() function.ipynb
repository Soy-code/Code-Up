    "def my_len(s) :\n",
    "    ret = 0 \n",
    "    while True :\n",
    "        try : \n",
    "            s[ret]   \n",
    "        except : \n",
    "            break \n",
    "        else :\n",
    "            ret += 1\n",
    "    return ret"
   ]
  },
